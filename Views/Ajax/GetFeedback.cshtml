@model BookRecommender.Models.AjaxViewModels.GetFeedback

<div class="container-fluid">
        <table class="table table-striped table-responsive">
            <thead>
                <tr>
                    <th class="col-sm-2">Name</th>
                    <th class="col-sm-8">Feedback</th>
                    <th class="col-sm-2">Email</th>
                </tr>
            </thead>
            <tbody>
                @foreach(var data in Model.Data){
                    <tr>
                        <td>
                            @data.Item1
                        </td>
                        <td>
                            @data.Item2
                        </td>
                        <td>
                            @data.Item3
                        </td>
                    </tr>
                }
            </tbody>
        </table>
</div>